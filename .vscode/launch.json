  "version": "0.2.0",
  "configurations": [
    {
      "name": "metrikr",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-metrikr",
      "program": "${workspaceFolder}/src/cli/bin/Debug/net6.0/metrikr.dll",
      "args": [
        "config.json"
      ],
      "cwd": "${workspaceFolder}/src/cli",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "sln-creator",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-sln",
      "program": "${workspaceFolder}/src/sln/bin/Debug/net6.0/sln.dll",
      "args": [
        "create-sln",
        "modules",
        "-i",
        "C:\\work\\joint-forces\\joint-forces\\backend\\modules",
        "-o",
        "C:\\work\\joint-forces\\joint-forces\\backend"
      ],
      "cwd": "${workspaceFolder}/src/sln",
      "console": "internalConsole",
      "stopAtEntry": false
    },
  ]
}