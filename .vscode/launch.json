  "version": "0.2.0",
  "configurations": [
    {
      "name": "metrikr",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/cli/bin/Debug/net5.0/metrikr.dll",
      "args": [
        "config.json"
